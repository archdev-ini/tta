{
    "kind": "collectionType",
    "collectionName": "programs",
    "info": {
        "singularName": "program",
        "pluralName": "programs",
        "displayName": "Programs"
    },
    "options": {
        "draftAndPublish": true
    },
    "attributes": {
        "title": {
            "type": "string",
            "required": true
        },
        "label": {
            "type": "string"
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "Upcoming",
                "Active",
                "Completed"
            ],
            "default": "Upcoming"
        },
        "featured": {
            "type": "boolean",
            "default": false
        },
        "description": {
            "type": "text"
        },
        "slug": {
            "type": "uid",
            "targetField": "title"
        },
        "content": {
            "type": "blocks"
        },
        "image": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images"
            ]
        },
        "seoTitle": {
            "type": "string"
        },
        "seoDescription": {
            "type": "text"
        },
        "seoImage": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images"
            ]
        }
    }
}